# Static information about the filesystems.
# See fstab(5) for details.

LABEL=root          	/         	ext4      	defaults,noatime,commit=60	0 1
LABEL=home          	/home         	ext4      	defaults,noatime,commit=60	0 2
LABEL=games          	/home/ivan/games         	ext4      	defaults,noatime,commit=60	0 2

LABEL=boot          	/boot     	vfat      	rw,relatime,fmask=0022,dmask=0022,codepage=437,iocharset=ascii,shortname=mixed,utf8,errors=remount-ro	0 2
LABEL=swap          	none      	swap      	defaults  	0 0

